                  <Button mnemonicParsing="false" text="Train Song" />
               </children>
            </VBox>
            <VBox maxWidth="1.7976931348623157E308">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Button mnemonicParsing="false" text="&lt;&lt;" />
